{
  "name": "spark-design-system",
  "description": "A pattern library authoring environment",
  "version": "3.0.0",
  "homepage": "https://github.com/sparkdesignsystem/spark-design-system",
  "author": "Quicken Loans",
  "dependencies": {
    "@angular/common": "^6.1.4",
    "@angular/core": "^6.1.4",
    "@sparkdesignsystem/spark": "^11.0.0",
    "@sparkdesignsystem/spark-angular": "^9.0.0",
    "@sparkdesignsystem/spark-react": "^1.0.1",
    "classnames": "^2.2.6",
    "gatsby": "^2.4.3",
    "gatsby-image": "^2.0.41",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-offline": "^2.1.0",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sharp": "^2.0.37",
    "gatsby-source-filesystem": "^2.0.37",
    "gatsby-transformer-sharp": "^2.1.19",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-router-dom": "^5.0.0",
    "shortid": "^2.2.14",
    "stylelint": "^10.0.1"
  },
  "devDependencies": {
    "@angular-devkit/core": "^8.0.1",
    "@angular/animations": "^6.1.4",
    "@angular/compiler": "^6.1.4",
    "@angular/compiler-cli": "^6.1.10",
    "@angular/forms": "^6.1.4",
    "@angular/platform-browser": "^6.1.4",
    "@angular/platform-browser-dynamic": "^6.1.4",
    "@angular/router": "^6.1.4",
    "@babel/core": "^7.4.4",
    "@storybook/addon-a11y": "^5.1.1",
    "@storybook/addon-actions": "^5.1.1",
    "@storybook/addon-info": "^5.1.1",
    "@storybook/addon-knobs": "^5.1.1",
    "@storybook/addon-links": "^5.1.1",
    "@storybook/addon-storysource": "^5.1.1",
    "@storybook/addons": "^5.1.1",
    "@storybook/angular": "^5.1.1",
    "@storybook/html": "^5.1.1",
    "@storybook/react": "^5.1.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-react": "^7.13.0",
    "prop-types": "^15.7.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint-scss": "^3.7.0",
    "typescript": "^3.5.1",
    "zone.js": "^0.8.29"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "build-storybook:html": "node node_modules/@storybook/html/bin/build -c .storybook-html -o ./static/html",
    "build-storybook:react": "node node_modules/@storybook/react/bin/build -c .storybook-react -o ./static/react",
    "build-storybook:angular": "node node_modules/@storybook/angular/bin/build -c .storybook-angular -o ./static/angular",
    "storybook:react": "node node_modules/@storybook/react/bin/index -p 9001 -c .storybook-react",
    "storybook:html": "node node_modules/@storybook/html/bin/index -p 8001 -c .storybook-html",
    "storybook:angular": "node node_modules/@storybook/angular/bin/index -p 7001 -c .storybook-angular",
    "storybook:all": "npm run storybook:html && npm run build-storybook:react && npm run build-storybook:angular"
  },
  "lint-staged": {
    "*.scss": [
      "stylelint --syntax scss",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sparkdesignsystem/spark-design-system"
  },
  "bugs": {
    "url": "https://github.com/sparkdesignsystem/spark-design-system/issues"
  }
}
